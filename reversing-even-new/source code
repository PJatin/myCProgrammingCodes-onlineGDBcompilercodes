#include<stdio.h>
#include<stdlib.h>

struct node{
    int data;
    struct node *next;
}*head,*temp,*p,*even,*odd;

struct node *createNode(struct node *head,int data){
    temp=(struct node *)malloc(sizeof(struct node));
    temp->data=dat;
    temp->next=NULL;
    if(head=NULL)
    {
        head=temp;
    }
    else
    {
        if(head==even)
        {
            temp->next=head;
            head=temp;
        }
        else
        {
            p=head;
            while(p->next!=NULL)
            p=p->next;
            p->next=temp;
        }
    }
}